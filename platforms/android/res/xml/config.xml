<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fdio.smarthome" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>fdi-o-media</name>
    <description>
       Saklar dan media player  untuk os semua openwrt 
    </description>
    <author email="fdiyadhibiermann@gmail.com" href="http://felexindo.online">
        Friyadhi Biermann
    </author>
    <content src="index.html" />
    <icon src="www/img/fdi.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="permissions" value="none" />
</widget>
